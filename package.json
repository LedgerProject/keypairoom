{
  "name": "@dyne/keypairoom",
  "version": "1.0.0",
  "description": "Keypair library",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rm -rf ./lib",
    "copy-files": "cp -r ./src/zencode/ ./lib/zencode && cp -r ./src/props/ ./lib/props"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LedgerProject/keypairoom.git"
  },
  "keywords": [
    "keypair",
    "zencode"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "zenroom": "^2.2.0-e4ecf22"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ðŸš€ ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
